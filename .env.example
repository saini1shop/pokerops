# PokerOps Environment Configuration
# Copy this file to .env and update with your actual values

# Application
APP_ENV=development
DEV_MODE=true
DEV_OTP=123456

# Database
DB_HOST=localhost
DB_NAME=pokerops
DB_USER=pokerops_app
DB_PASS=your_db_password_here

# AiSensy WhatsApp (optional for dev)
AISENSY_API_KEY=your_aisensy_api_key
AISENSY_WEBHOOK_SECRET=your_webhook_secret
